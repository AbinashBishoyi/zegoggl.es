---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>ze goggles</title>
 <link href="http://zegoggl.es/feed.atom" rel="self"/>
 <link href="http://zegoggl.es/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://zegoggl.es/</id>
 <author>
   <name>Jan Berkel</name>
   <email>jan.berkel@gmail.com</email>
 </author>

 {% for post in site.posts limit: 15 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://zegoggl.es{{ post.url }}"/>
   <published>{{ post.date | date_to_xmlschema }}</published>
   {% comment %} <updated>{{ post.date | date_to_xmlschema }}</updated> {% endcomment %}
   <id>http://zegoggl.es{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
