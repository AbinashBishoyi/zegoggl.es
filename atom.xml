---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>zegoggles</title>
 <link href="http://zegoggle.com/atom.xml" rel="self"/>
 <link href="http://zegoggle.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://zegoggles.com/</id>
 <author>
   <name>Jan Berkel</name>
   <email>jan@zegoggles.com</email>
 </author>

 {% for post in site.posts limit: 15 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://zegoggles.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://zegoggles.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>